
fn selectionSort(array){
    for(i; i < len(array)-1; i++) {
        min_idx = i

        for(let l=i+1; l < len(array); l++){
            if array[l] < array[min_idx]:
                min_idx = l
        }

        buf = [array[min_idx], array[i]]

        array[i] = buf[0]
        array[min_idx] = buf[1]
    }

    return array
}

disorganised_data = [3, 7, 2, 1, 0, 5, 6, 4, 9, 8] 

organised = selectionSort(disorganised_data)
print(organised)

